        <profile>
            <id>baidu</id>
            <repositories>
                <repository>
                    <id>baidu-nexus</id>
                    <url>http://maven.baidu-int.com/nexus/content/groups/public</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>baidu-nexus-snapshot</id>
                    <url>http://maven.baidu-int.com/nexus/content/groups/public-snapshots</url>
